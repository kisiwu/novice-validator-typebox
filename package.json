{
  "name": "@novice1/validator-typebox",
  "private": false,
  "version": "1.0.0",
  "description": "Typebox validator to use with @novice1/routing.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "doc": "typedoc",
    "build": "tsc && node ./scripts/buildDT.mjs",
    "lint": "eslint .",
    "test": "cross-env TS_NODE_FILES=true kaukau --require ts-node/register --config kaukau-src.config.mjs",
    "test:lib": "kaukau --config kaukau-lib.config.mjs",
    "format": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --write ./src",
    "format:check": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --check ./src",
    "format:staged": "cross-env NODE_OPTIONS=--experimental-strip-types lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisiwu/novice-validator-typebox.git"
  },
  "keywords": [
    "express",
    "middleware",
    "typebox",
    "validator"
  ],
  "author": "demingongo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kisiwu/novice-validator-typebox/issues"
  },
  "homepage": "https://github.com/kisiwu/novice-validator-typebox#readme",
  "dependencies": {
    "@novice1/logger": "^1.6.1",
    "extend": "^3.0.2",
    "tslib": "^2.8.1",
    "typebox": "^1.0.81"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@novice1/routing": "^2.1.0",
    "@stylistic/eslint-plugin": "^5.8.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/chai": "^5.2.3",
    "@types/express": "^5.0.6",
    "@types/extend": "^3.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "chai": "^4.5.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "express": "^5.2.1",
    "globals": "^17.3.0",
    "kaukau": "^4.2.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
